{
  "name": "utf-8",
  "description": "Encode/decode UTF8.",
  "version": "1.0.0",
  "author": "Nicolas Froidure",
  "repository": {
    "type": "git",
    "url": "http://github.com/nfroidure/UTF8.js.git"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "string.fromcodepoint": "^0.2.1",
    "string.prototype.codepointat": "^0.2.0"
  },
  "devDependencies": {
    "mocha": "~2.2.5",
    "mocha-lcov-reporter": "0.0.2",
